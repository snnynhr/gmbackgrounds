{
  "manifest_version": 2,

  "name": "GmailBackgrounds",
  "description": "Random HQ backgrounds for gmail.",
  "version": "0.0.0.1",
  "permissions": [
  "tabs",
	"activeTab",
	"http://*/*",
	"https://*/*"  ],
  "content_scripts" : [
  {
    "matches": ["*://mail.google.com/*", "*://www.gmail.com/"],
    "js": ["util/jquery.min.js", "js/gmedit.js"],
    "run_at": "document_start"
  }
  ],
  "web_accessible_resources": ["backgrounds/*"]
}